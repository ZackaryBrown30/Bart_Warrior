<% for (var i = 0; i < North.root.station[0].etd.length; i++) { %>
<div class="col-md-5 m-1 rounded bg-warning text-light">
  North
  <h1>
    <span class="text-primary">
      <%= North.root.station[0].etd[i].destination %></span
    >
  </h1>
  <h2>
    in:
    <span class="text-primary">
      <% for ( J = 0; J < North.root.station[0].etd[i].estimate.length; J++) {
      %> <%= North.root.station[0].etd[i].estimate[J].minutes %> <% if ( J <
      North.root.station[0].etd[i].estimate.length-1){ %> , <% } %> <% } %>
    </span>
  </h2>
</div>
<% } %>
